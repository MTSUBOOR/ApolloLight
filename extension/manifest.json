{
    "name": "Apollo Light",
    "description": "Helping shed light on biased articles in the news.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage", "activeTab", "scripting"],
      "action": {
        "default_popup": "index.html"
      },
      "default_icon": {
        "16": "/images/Logo.png",
        "48": "/images/Logo.png",
        "128": "/images/Logo.png"
      },
      "icons": {
        "16": "/images/Logo.png",
        "48": "/images/Logo.png",
        "128": "/images/Logo.png"
      }
  
  }